<script>
    $('.btnRedondo').click(function (event) {
        event.preventDefault();
        $('#loadingAjaxLug').fadeToggle(250);
        idActividad = $(this).data('idactividad');

        var postDataLug = 'idactividad=' + idActividad;

        $.ajax({
            type: 'POST',
            dataType: 'html',
            cache: true,
            url: '/ajax/actividades',
            data: postDataLug,
            success: function (htmlLug) {

                $('#areaLugares').html(htmlLug).hide().waitForImages(function () {
                    $('#areaLugares').show();
                    $('#loadingAjaxLug').fadeToggle(250);
                    $('body').addClass("sinScroll");
                    //BG Img
                    /*$(".bgLug").interactive_bg({
                     strength: 25,
                     scale: 1.05,
                     animationSpeed: "100ms"
                     });*/
                    $(window).resize(function () {
                        $(".bgLug > .ibg-bg").css({
                            width: $(window).outerWidth(),
                            height: $(window).outerHeight()
                        });
                    });
                    $("#bgLug,#nombreLug,#descripcionLug").css('cursor', 'url(img/cursor.png),auto');

                    $('#closeLug').click(function () {
                        $('body').removeClass("sinScroll");
                        $('#areaLugares').fadeOut(250, function () {
                            $('#areaLugares').html('');
                        });
                        return false;
                    });
                    $('.btnFullImg').not('#closeLug').click(function () {
                        $('.fullImgOcultar').fadeToggle();
                    });
                    //BG Img

                });

            } //success
        }); //ajax

    });
</script>